<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.labour.dao.LoginDao">

    <select id="selectUser" resultType="com.labour.entity.User" parameterType="java.lang.String">
		select user_id,user_name,password,name,create_user_id,create_user_name,create_time,update_time,status FROM user
		where user_name=#{user_name};
	</select>

</mapper>